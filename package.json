{
  "name": "compelo-ui",
  "version": "0.0.1",
  "author": "Florian Ehmke",
  "license": "MIT",
  "repository": "https://github.com/florianehmke/compelo-ui",
  "scripts": {
    "build": "react-scripts build",
    "build:ci": "run-s verify build",
    "eject": "react-scripts eject",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx}\"",
    "generate": "graphql-codegen --config codegen.js",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "verify": "run-s lint format:check"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.10",
    "@types/uuid": "^7.0.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.3",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.10.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.10.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-select": "^3.0.8",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "~3.7.2",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "npm-run-all": "^4.1.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^1.19.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
